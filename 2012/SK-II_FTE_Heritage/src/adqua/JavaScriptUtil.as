package com.adqua.utils {	import flash.external.*;	import com.adqua.utils.*;	public class JavaScriptUtil {		static public function call( $jsFun:String, ... args ):void {			if( NetUtil.isBrowser() ) {		       		if( ExternalInterface.available ) ExternalInterface.call.apply( null, [ $jsFun ].concat( args ) );			} else {				trace( "[ "+$jsFun+" ] "+args );			}		}		static public function callBack( $jsFun:String, $cbkFn:Function ):void {			if( ExternalInterface.available ) ExternalInterface.addCallback( $jsFun, $cbkFn );		}		static public function alert( $str:String ):void {			if( NetUtil.isBrowser() ) ExternalInterface.call( "alert", $str );			else trace( $str );		}	}}