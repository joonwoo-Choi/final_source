package orpheus.controls.Tab {	import flash.events.Event;		/**	 * @author philip	 * MZ package 1.0	 */	public class TabEvent extends Event 	{		public static const CHANGE:String = "tabChanged";		public static const CLICK:String = "btnClick";		public var selectTab:*;		public function TabEvent(type:String, num:*, bubbles:Boolean = false, cancelable:Boolean = false)		{			selectTab = num;			super(type, bubbles, cancelable);		}				override public function clone():Event		{			return new TabEvent(type, selectTab, bubbles, cancelable);		}	}}