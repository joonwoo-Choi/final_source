package adqua.control.flvPlayer.events {	import flash.events.Event;		/**	 * @author philip	 */	public class SeekEvent extends Event 	{		public var per:Number;		public static const SEEK:String = "SEEK";		public var state:String;		public function SeekEvent(type:String, percent:Number, state:String = "", bubbles:Boolean = false, cancelable:Boolean = false)		{			this.state = state;			this.per = percent;			super(type, bubbles, cancelable);		}				override public function clone():Event		{			return new SeekEvent(type, per, state, bubbles, cancelable);		}	}}