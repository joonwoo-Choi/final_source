package com.adqua.control.flvPlayer.events {	import flash.events.Event;	/**	 * @author philip	 */	public class MovieStatusEvent extends Event 	{		public var code:*;		public static const PLAY_STATUS_EVENT:String = "PLAY_STATUS_EVENT";		public static const STATUS_EVENT:String = "STATUS_EVENT";		public static const CREATE_NETSTREAM:String = "CREATE_NETSTREAM";		public static const CREATE_VIDEO:String = "CREATE_VIDEO";		public static const COMPLETE:String = "PLAY_COMPLETE";		public static const START:String = "PLAY_START";				public static const NETSTREAM_ALL_STOP:String = "NetStream.All.Stop";		public static const NETSTREAM_PLAY_STOP:String = "NetStream.Play.Stop";		public static const NETSTREAM_PLAY_COMPLETE:String = "NetStream.Play.Complete";		public static const NETSTREAM_PLAY_START:String = "NetStream.Play.Start";		public static const NETSTREAM_BUFFER_EMPTY:String = "NetStream.Buffer.Empty";		public static const NETSTREAM_BUFFER_FULL:String = "NetStream.Buffer.Full";		public static const NETSTREAM_BUFFER_FLUSH:String = "NetStream.Buffer.Flush";		public static const NETSTREAM_SEEK_FAILED:String = "NetStream.Seek.Failed";		public static const NETSTREAM_SEEK_NOTIFY:String = "NetStream.Seek.Notify";		public static const NETSTREAM_SEEK_INVALIDTIME:String = "NetStream.Seek.InvalidTime";		public static const MUTE:String = "MUTE";		public static const PAUSE:String = "PAUSE";		public static const RESUME:String = "RESUME";		public static const STOP:String = "STOP";		public static const BLANK_SOURCE:String = "BLANK_SOURCE";				public function MovieStatusEvent(type:String, code:* = "", bubbles:Boolean = false, cancelable:Boolean = false)		{			this.code = code;			super(type, bubbles, cancelable);		}		override public function clone():Event		{			return new MovieStatusEvent(type, code, bubbles, cancelable);		}	}}