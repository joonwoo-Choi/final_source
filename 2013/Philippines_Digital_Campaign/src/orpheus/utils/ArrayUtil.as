package orpheus.utils {	/**	 * @author philip	 */	public class ArrayUtil 	{		//배열 섞기		public static function shuffle(arr:Array):void		{			var tmp:Array = arr.concat();			var cnt:uint = 0;			while(tmp.length)			{				arr[cnt] = tmp.splice(Math.floor(Math.random() * tmp.length), 1)[0];				cnt++;			}		}				public static function compare(arr1:Array, arr2:Array, order:Boolean = true):Boolean		{			if (arr1.length != arr2.length) return false;						var i:uint;			var cnt:uint;						if (order)			{				for(i = 0; i < arr1.length; i++)				{					if (arr1[i] != arr2[i]) return false;				}				return true;			}			else			{				var tmpArr:Array = arr2.concat();								for(i = 0; i < arr1.length; i++)				{					for(var j:uint = 0; j < tmpArr.length; j++)					{						if (arr1[i] == tmpArr[j])						{							cnt++;							tmpArr.splice(j, 1);							break;						}					}				}				return (cnt == arr1.length);			}		}	}}